{
  "name": "@stackspot/cdk-vpc",
  "version": "0.1.1",
  "description": "Component to create a VPC.",
  "keywords": [
    "aws",
    "cdk",
    "vpc"
  ],
  "homepage": "https://www.stackspot.com/studios/Z2l0aHViOjpzdGFjay1zcG90L3NreW5ldC1zdHVkaW8=",
  "bugs": "https://github.com/stack-spot/vpc-jsii-component/issues",
  "license": "Apache-2.0",
  "author": {
    "name": "StackSpot",
    "email": "contact@stackspot.com",
    "url": "https://www.stackspot.com"
  },
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/stack-spot/vpc-jsii-component.git"
  },
  "scripts": {
    "build": "jsii --verbose --fail-on-warnings",
    "build:watch": "npm run build -- --watch",
    "format": "prettier --check .",
    "format:fix": "prettier --write .",
    "lint": "eslint --ext=ts --max-warnings=0 .",
    "lint:fix": "npm run lint -- --fix",
    "package": "jsii-pacmak --verbose --validate-assemblies",
    "package:dotnet": "jsii-pacmak --target=dotnet --verbose --validate-assemblies",
    "package:js": "jsii-pacmak --target=js --verbose --validate-assemblies",
    "test": "jest",
    "test:watch": "npm test -- --watch"
  },
  "devDependencies": {
    "@types/jest": "^27.0.0",
    "@types/node": "^17.0.0",
    "@typescript-eslint/eslint-plugin": "^5.16.0",
    "@typescript-eslint/parser": "^5.16.0",
    "aws-cdk-lib": "^2.0.0",
    "constructs": "^10.0.0",
    "eslint": "^8.11.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.25.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^27.0.0",
    "jsii": "^1.55.0",
    "jsii-pacmak": "^1.55.0",
    "prettier": "^2.6.0",
    "ts-jest": "^27.0.0",
    "typescript": "~4.6.0"
  },
  "peerDependencies": {
    "aws-cdk-lib": "^2.0.0",
    "constructs": "^10.0.0"
  },
  "jest": {
    "collectCoverage": true,
    "coverageThreshold": {
      "global": {
        "branches": 58,
        "functions": 80,
        "lines": 80,
        "statements": 80
      }
    },
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/test"
    ],
    "testMatch": [
      "**/*.test.ts"
    ],
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "verbose": true
  },
  "stability": "stable",
  "jsii": {
    "outdir": "dist",
    "versionFormat": "full",
    "targets": {
      "dotnet": {
        "namespace": "StackSpot.Cdk.Vpc",
        "packageId": "StackSpot.Cdk.Vpc",
        "iconUrl": "https://www.nuget.org/profiles/stackspot/avatar"
      }
    }
  }
}
